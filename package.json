{
	"name": "piwoby",
	"version": "0.0.1",
	"description": "PIWO.by SvelteKit Blog",
	"scripts": {
		"dev": "vite dev --port 3030 --host",
		"dev:open": "vite dev --port 3030 --host --open",
		"start": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --port 3030 --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"keywords": [
		"sveltekit",
		"typescript",
		"supabase"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Saturnych/piwoby.git"
	},
	"bugs": {
		"url": "https://github.com/Saturnych/piwoby/issues"
	},
	"homepage": "https://piwo.by",
	"author": "Denis Glebko",
	"license": "CC0-1.0",
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.8",
		"@neoconfetti/svelte": "^2.2.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.18",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"github-slugger": "^2.0.0",
		"mdsvex": "^0.11.0",
		"mdsvex-relative-images": "^1.0.3",
		"postcss": "^8.4.35",
		"rehype-external-links": "^3.0.0",
		"rss": "^1.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
